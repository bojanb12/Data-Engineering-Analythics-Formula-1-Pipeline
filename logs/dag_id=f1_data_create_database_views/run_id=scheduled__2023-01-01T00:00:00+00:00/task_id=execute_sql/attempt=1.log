    WHERE
        "finish_position" <= 3
    GROUP BY
        "constructor_reference", "constructor_name", "podium_streak"
),
WinningStreaks AS (
    SELECT
        "constructor_reference",
        "constructor_name",
        COUNT(*) AS "streak_length",
        "win_streak"
    FROM
        RankedResults
    WHERE
        "finish_position" = 1
    GROUP BY
        "constructor_reference", "constructor_name", "win_streak"
)
SELECT
